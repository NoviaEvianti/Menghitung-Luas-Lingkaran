#include <iostream>
using namespace std;
int main()
{
  float l, r;
  float phi;
  
  phi = float (22)/7;
  
  
  cout << "=================================\n";
  cout << "Program Menghitung Luas Lingkaran\n";
  cout << "=================================\n";
  cout << "Masukkan panjang jari-jari lingkaran: ";
  cin >> r;
  l = phi*r*r;
  cout << "Luas Lingkaran adalah "<< (22)/7 << endl;
  
  
  return 0;
}
